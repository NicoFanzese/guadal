<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="4.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Guadal Encuesta</name>
    <description>
        Sistema de encuesta para editorial Guadal
    </description>
    <author email="leo@unidadmobile.com" href="http://unidadmobile.com">
        Leo Condori
    </author>
    <content src="index.html" />
    <icon src="app/192x192.png" />
    <splash src="app/1280x1920.png" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://leocondori.com.ar/*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="http://leocondori.com.ar/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="4000" />
</widget>
